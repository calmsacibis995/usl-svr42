cd /usr/X/lib/fonts/misc
rm -f 10x20.snf 12x24.snf 12x24rk.snf 5x8.snf 6x10.snf 6x13B.snf 6x9.snf 7x13.snf 7x13B.snf 7x14rk.snf 8x16.snf 8x16rk.snf 9x15.snf 9x15B.snf clB6x10.snf clB6x12.snf clB8x10.snf clB8x12.snf clB8x13.snf clB8x14.snf clB8x16.snf clB8x8.snf clB9x15.snf clI6x12.snf clI8x8.snf clR4x6.snf clR5x10.snf clR5x6.snf clR5x8.snf clR6x10.snf clR6x12.snf clR6x13.snf clR6x6.snf clR6x8.snf clR7x10.snf clR7x12.snf clR7x14.snf clR7x8.snf clR8x10.snf clR8x12.snf clR8x13.snf clR8x14.snf clR8x16.snf clR8x8.snf clR9x15.snf deccurs.snf decsess.snf nil2.snf olcursor.snf olgl10.snf olgl12.snf olgl14.snf olgl19.snf pc12x20.snf pc6x10.snf 

#
# update the fonts.dir and fonts.alias files
#
if [ -f /usr/X/lib/fonts/misc/dtxt.alias ]
then
    cp /usr/X/lib/fonts/misc/dtxt.alias /usr/X/lib/fonts/misc/fonts.alias
fi
if [ -f /usr/X/lib/fonts/75dpi/desk.alias ]
then
    cp /usr/X/lib/fonts/75dpi/desk.alias /usr/X/lib/fonts/75dpi/fonts.alias
fi
if [ -f /usr/X/lib/fonts/100dpi/desk.alias ]
then
    cp /usr/X/lib/fonts/100dpi/desk.alias /usr/X/lib/fonts/100dpi/fonts.alias
fi
/usr/X/bin/mkfontdir /usr/X/lib/fonts/misc
/usr/X/bin/mkfontdir /usr/X/lib/fonts/75dpi
/usr/X/bin/mkfontdir /usr/X/lib/fonts/100dpi

#
# re-run xset if the package is removed will in graphics
#
TTY=`/bin/tty`
echo "$TTY" | grep "/dev/pts" > /dev/null 2>&1
RET_VAL=$?
[ "$RET_VAL" = "0" ] && LD_LIBRARY_PATH=/usr/X/lib /usr/X/bin/xset -display unix:0 fp rehash >/dev/null 2>&1

exit 0
