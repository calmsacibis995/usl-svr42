rm -f charB08.snf charB10.snf charB12.snf charB14.snf charB18.snf charB24.snf charBI08.snf charBI10.snf charBI12.snf charBI14.snf charBI18.snf charBI24.snf charI08.snf charI10.snf charI12.snf charI14.snf charI18.snf charI24.snf charR08.snf charR10.snf charR12.snf charR14.snf charR18.snf charR24.snf courB08.snf courB10.snf courB14.snf courB18.snf courB24.snf courBO08.snf courBO10.snf courBO14.snf courBO18.snf courBO24.snf courO08.snf courO10.snf courO14.snf courO18.snf courO24.snf courR08.snf courR10.snf courR14.snf courR18.snf courR24.snf helvB08.snf helvB10.snf helvB14.snf helvB18.snf helvB24.snf helvBO08.snf helvBO10.snf helvBO12.snf helvBO14.snf helvBO18.snf helvBO24.snf helvO08.snf helvO10.snf helvO14.snf helvO18.snf helvO24.snf helvR08.snf helvR10.snf helvR14.snf helvR18.snf helvR24.snf luBIS08.snf luBIS10.snf luBIS14.snf luBIS18.snf luBIS19.snf luBIS24.snf luBS08.snf luBS10.snf luBS18.snf luBS19.snf luBS24.snf luIS08.snf luIS10.snf luIS14.snf luIS18.snf luIS19.snf luIS24.snf luRS08.snf luRS10.snf luRS14.snf luRS18.snf luRS19.snf luRS24.snf lubB08.snf lubB10.snf lubB12.snf lubB14.snf lubB18.snf lubB19.snf lubB24.snf lubBI08.snf lubBI10.snf lubBI12.snf lubBI14.snf lubBI18.snf lubBI19.snf lubBI24.snf lubI08.snf lubI10.snf lubI12.snf lubI14.snf lubI18.snf lubI19.snf lubI24.snf lubR08.snf lubR10.snf lubR12.snf lubR14.snf lubR18.snf lubR19.snf lubR24.snf lutBS08.snf lutBS10.snf lutBS14.snf lutBS18.snf lutBS19.snf lutBS24.snf lutRS08.snf lutRS10.snf lutRS14.snf lutRS18.snf lutRS19.snf lutRS24.snf ncenB08.snf ncenB10.snf ncenB12.snf ncenB14.snf ncenB18.snf ncenB24.snf ncenBI08.snf ncenBI10.snf ncenBI12.snf ncenBI14.snf ncenBI18.snf ncenBI24.snf ncenI08.snf ncenI10.snf ncenI12.snf ncenI14.snf ncenI18.snf ncenI24.snf ncenR08.snf ncenR10.snf ncenR12.snf ncenR14.snf ncenR18.snf ncenR24.snf symb08.snf symb10.snf symb12.snf symb14.snf symb18.snf symb24.snf tech14.snf techB14.snf term14.snf termB14.snf timB08.snf timB10.snf timB12.snf timB14.snf timB18.snf timB24.snf timBI08