!PKGINST=atm

!default 0755 bin bin

d enduser /usr ? ? ?

d enduser /usr/X
d enduser /usr/X/lib
d enduser /usr/X/lib/fonts
d enduser /usr/X/lib/fonts/mitType1
d enduser /usr/X/lib/fonts/mitType1/afm

!default 0644 bin bin

f enduser /usr/X/lib/libatm.so

f enduser /usr/X/lib/fonts/mitType1/UTBI____.pfa
f enduser /usr/X/lib/fonts/mitType1/UTB_____.pfa
f enduser /usr/X/lib/fonts/mitType1/UTI_____.pfa
f enduser /usr/X/lib/fonts/mitType1/UTRG____.pfa
v enduser /usr/X/lib/fonts/mitType1/fonts.dir
f enduser /usr/X/lib/fonts/mitType1/afm/UTBI____.afm
f enduser /usr/X/lib/fonts/mitType1/afm/UTB_____.afm
f enduser /usr/X/lib/fonts/mitType1/afm/UTI_____.afm
f enduser /usr/X/lib/fonts/mitType1/afm/UTRG____.afm
