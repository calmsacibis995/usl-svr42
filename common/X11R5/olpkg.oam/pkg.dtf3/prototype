!PKGINST=dtf3

!default 0755 bin bin

d enduser /usr ? ? ?

d enduser /usr/X
d enduser /usr/X/lib
d enduser /usr/X/lib/fonts
d enduser /usr/X/lib/fonts/folio

!default 0644 bin bin

f enduser /usr/X/lib/libfolio.so

f enduser /usr/X/lib/fonts/folio/LcdBr.f3b
f enduser /usr/X/lib/fonts/folio/LcdBrDB.f3b
f enduser /usr/X/lib/fonts/folio/LcdBrDBI.f3b
f enduser /usr/X/lib/fonts/folio/LcdBrI.f3b
f enduser /usr/X/lib/fonts/folio/LcdSanB.f3b
f enduser /usr/X/lib/fonts/folio/LcdSanBI.f3b
f enduser /usr/X/lib/fonts/folio/LcdSanI.f3b
f enduser /usr/X/lib/fonts/folio/LcdSanT.f3b
f enduser /usr/X/lib/fonts/folio/LcdSanTB.f3b
f enduser /usr/X/lib/fonts/folio/LcdSans.f3b
f enduser /usr/X/lib/fonts/folio/fonts.dir
f enduser /usr/X/lib/fonts/folio/fonts.scale=/usr/X/lib/fonts/folio/fonts.dir
