# This is a sample profile to set some necessary 
# window environment parameters.  It may be 
# tailored to fit individual needs.
 
PATH=$PATH:/usr/X/bin
DISPLAY=unix:0
FDIR=/usr/X/lib/fonts
export DISPLAY PATH

