#ident	"@(#)scsi-cmds:i386at/cmd/scsi-cmds/mkdev.d/cdrom1	1.4"
#ident  "$Header: $"
QUERY	-
POSTMSG	-
ALIAS	cdrom
DGRP	scsicdrom
ATTR	type="cdrom" scsi="true" volume="CD" removable="true" pdimkdtab="true" bklib="SCSI" bufsize="2048" desc="SCSI CD-Rom Drive $N$" display="true"
DATA
key	minor	mode	/dev/cdrom/	/dev/rcdrom/	/dev/cdrom/	/dev/rcdrom/
MO	0+D*S	0666	cCtTlL		cCtTlL		-		cdromN
