#ident	"@(#)oamintf:i386/cmd/oamintf/machinemgmt/Prototype	1.2"
#ident  "$Header: Prototype 1.1 91/08/28 $"
!MENU=/usr/sadm/sysadm/menu
!INTFBIN=/usr/sadm/sysadm/bin

d intf $MENU/machinemgmt 0755 bin bin
v intf $MENU/machinemgmt/machine.menu 0644 bin bin
d intf $MENU/machinemgmt/power 0755 bin bin
f intf $MENU/machinemgmt/power/Text.power 0644 bin bin
f intf $MENU/machinemgmt/power/Form.power 0644 bin bin
d intf $MENU/machinemgmt/reboot 0755 bin bin
f intf $MENU/machinemgmt/reboot/Text.reboot 0644 bin bin
f intf $MENU/machinemgmt/reboot/Form.reboot 0644 bin bin
d intf $MENU/machinemgmt/whoson 0755 bin bin
f intf $MENU/machinemgmt/whoson/Text.whoson 0644 bin bin
f intf $INTFBIN/chgstate 0755 bin bin
f intf $MENU/machinemgmt/Help 0644 bin bin
f intf $MENU/machinemgmt/power/Help 0644 bin bin
f intf $MENU/machinemgmt/reboot/Help 0644 bin bin
f intf $MENU/machinemgmt/whoson/Help 0644 bin bin
